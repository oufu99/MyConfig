<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>input</Title>
    <Author>Microsoft Corporation</Author>
    <Shortcut>input</Shortcut>
    <Description>输入元素的标记段</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
      <SnippetType>SurroundsWith</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>type</ID>
        <ToolTip>类型</ToolTip>
        <Default>type</Default>
      </Literal>
      <Literal>
        <ID>name</ID>
        <ToolTip>名称</ToolTip>
        <Default>name</Default>
      </Literal>
      <Literal>
        <ID>selected</ID>
        <ToolTip>值</ToolTip>
        <Default></Default>
      </Literal>
    </Declarations>
    <Code Language="html"><![CDATA[<input type="$type$"  value="$selected$" />$end$]]></Code>
  </Snippet>
</CodeSnippet>
